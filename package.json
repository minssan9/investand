    "docker:down": "./scripts/docker-utils.sh down",
    "docker:clean": "./scripts/docker-utils.sh clean",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install && cd ..",
    "update:all": "npm update && cd backend && npm update && cd ../frontend && npm update && cd .."
