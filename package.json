    "restart:staging": "ENVIRONMENT=staging ./scripts/fg-manager.sh restart staging",
    "status": "./scripts/fg-manager.sh status",
    "logs": "./scripts/fg-manager.sh logs",
    "logs:backend": "./scripts/fg-manager.sh logs backend",
    "logs:frontend": "./scripts/fg-manager.sh logs frontend",
    "build": "./scripts/fg-manager.sh build",
    "test": "./scripts/fg-manager.sh test",
    "health": "./scripts/fg-manager.sh health",
    "backup": "./scripts/fg-manager.sh backup",
    "clean": "./scripts/fg-manager.sh clean",
    "deploy": "./scripts/fg-manager.sh deploy",
    "deploy:prod": "ENVIRONMENT=production ./scripts/fg-manager.sh deploy production",
