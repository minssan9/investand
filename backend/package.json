{
  "name": "kospi-fg-index-backend",
  "version": "1.0.0",
  "description": "KOSPI Fear & Greed Index Backend API Server",
  "main": "dist/server.js",
  "scripts": {
    "start": "dotenv -e ../.env -- node dist/server.js",
    "start:prod": "NODE_ENV=production dotenv -e ../.env -- node dist/server.js",
    "start:scheduler": "dotenv -e ../.env -- node -r ts-node/register -r tsconfig-paths/register -e \"require('./src/services/domain/BatchProcessingService').BatchProcessingService.initialize()\"",
    "dev": "dotenv -e ../.env -- nodemon --exec \"ts-node -r tsconfig-paths/register\" src/server.ts",
    "build:prod": "NODE_ENV=production dotenv -e ../.env -- tsc",
    "messaging:send-feargreed": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register src/scripts/sendTestMessage.ts feargreed",
    "messaging:send-summary": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register src/scripts/sendTestMessage.ts summary",
    "collect:data": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register -e \"import('./src/services/domain/DataCollectionService').then(m => m.DataCollectionService.collectDailyData(new Date().toISOString().split('T')[0], {}))\"",
    "collect:historical": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register -e \"import('./src/services/domain/DataCollectionService').then(m => m.DataCollectionService.collectHistoricalData('2025-01-16', '2025-01-31', {}))\"",
    "collect:dart": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register src/scripts/collectDartData.ts",
    "collect:dart-daily": "npm run collect:dart today",
    "collect:dart-yesterday": "npm run collect:dart yesterday",
    "collect:dart-business": "npm run collect:dart last-business",
    "messaging:start": "dotenv -e ../.env -- npx ts-node -r tsconfig-paths/register -e \"import('./src/services/messaging/MessagingController').then(m => m.MessagingController.getInstance().initialize())\"",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx ts-node -r tsconfig-paths/register prisma/seed.ts", 
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "kospi",
    "fear-greed-index",
    "stock-market",
    "api",
    "typescript",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.0.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "iconv-lite": "^0.6.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.9.8",
    "prisma": "^5.7.1",
    "rate-limiter-flexible": "^4.0.1",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.20.1",
    "web-push": "^3.6.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
